<h3><%= t('admin.index.welcome') + "  " + current_staff.staff_name %></h3>
<%= link_to t('admin.index.logout'), signout_path, method: :delete %>
<% if flash.keys.any? %>
  <% [:notice, :error].each do |t| %>
    <%= flash[t] %>
  <% end %>
<% end %>
<ul>
	<%= @staffs.each do |staff| %>
	<li><%= staff.staff_name %></li>
	<% end %>
</ul>